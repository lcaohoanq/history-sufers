# History Sufers Server Configuration

# Server Environment
NODE_ENV=production

# Server Port
# Default: 3000
PORT=3000

# Maximum number of players allowed per room
# Default: 50
# Examples: 10, 25, 50, 100
MAX_PLAYERS=50

# Enable or disable room browser feature
# Default: true
ENABLE_ROOM_BROWSER=true

# Room cleanup interval in milliseconds (5 minutes)
# Default: 300000 (5 minutes)
ROOM_CLEANUP_INTERVAL=300000

# Docker Configuration (for Docker deployment)
DOCKER_USERNAME=lcaohoanq
VERSION=latest

